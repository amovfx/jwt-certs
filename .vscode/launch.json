  "version": "0.2.0",
  "configurations": [
    {
      "name": "Make 512 keys",
      "type": "python",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "env": { "PYTHONPATH": "${cwd}" }
    },

    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": { "PYTHONPATH": "${cwd}" },
      "envFile": "${workspaceFolder}/.env",
    }
  ]
}
